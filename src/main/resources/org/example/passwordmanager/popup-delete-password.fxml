<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="108.0" prefWidth="257.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.passwordmanager.UserInterface.PopupDeletePassword">
   <VBox alignment="TOP_CENTER" layoutX="34.0" layoutY="24.0" prefHeight="0.0" prefWidth="189.0">
      <Label text="Are you sure?">
         <VBox.margin>
            <Insets bottom="20.0"/>
         </VBox.margin>
      </Label>
      <HBox alignment="TOP_CENTER" prefHeight="0.0" prefWidth="189.0">
         <Button mnemonicParsing="false" onAction="#onConfirmButtonClick" text="Confirm"/>
         <Button mnemonicParsing="false" onAction="#onCancelButtonClick" text="Cancel">
            <HBox.margin>
               <Insets left="40.0"/>
            </HBox.margin>
         </Button>
      </HBox>
   </VBox>
</AnchorPane>
