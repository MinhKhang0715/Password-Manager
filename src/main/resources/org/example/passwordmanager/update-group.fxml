<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="202.0" prefWidth="547.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.passwordmanager.UserInterface.UpdateGroupController">
   <children>
      <VBox alignment="CENTER" layoutX="39.0" layoutY="37.0" prefHeight="128.0" prefWidth="470.0">
         <children>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="333.0" spacing="155.0">
               <children>
                  <Label text="Group name" />
                  <TextField fx:id="txt_groupName" prefHeight="24.0" prefWidth="173.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="379.0" spacing="20.0">
               <children>
                  <CheckBox fx:id="chk_changePassword" mnemonicParsing="false" prefHeight="18.0" prefWidth="219.0" text="Change password" />
                  <PasswordField fx:id="txt_groupPassword" editable="false" />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="333.0" spacing="180.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onUpdateButtonClick" text="Update" />
                  <Button mnemonicParsing="false" onAction="#onCancelButtonClick" text="Cancel" />
               </children>
            </HBox>
            <Label fx:id="txt_emptyName" />
         </children>
      </VBox>
   </children>
</AnchorPane>
