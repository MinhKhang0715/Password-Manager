<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="151.0" prefWidth="461.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.passwordmanager.UserInterface.PopupDeleteGroup">
   <children>
      <VBox layoutX="32.0" layoutY="28.0" prefHeight="96.0" prefWidth="398.0">
         <children>
            <VBox alignment="CENTER" prefHeight="16.0" prefWidth="324.0">
               <children>
                  <Label text="Deleting group will also delete all saved passwords in it">
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                  </Label>
                  <Label text="This operation cannot be undone. Are you sure?" />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </VBox>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="324.0" spacing="100.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onDeleteButtonClick" text="Sure, delete it" />
                  <Button mnemonicParsing="false" onAction="#onCancelButtonClick" text="Cancel" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
