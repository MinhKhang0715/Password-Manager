<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="202.0" prefWidth="374.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.passwordmanager.UserInterface.UpdateGroupController">
   <children>
      <VBox alignment="CENTER" layoutX="21.0" layoutY="45.0" prefHeight="56.0" prefWidth="333.0">
         <children>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="333.0" spacing="70.0">
               <children>
                  <Label text="Group name" />
                  <TextField fx:id="txt_groupName" prefHeight="24.0" prefWidth="173.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="333.0" spacing="90.0">
               <children>
                  <Label text="Password" />
                  <PasswordField fx:id="txt_groupPassword" />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="333.0" spacing="100.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onUpdateButtonClick" text="Update" />
                  <Button mnemonicParsing="false" onAction="#onCancelButtonClick" text="Cancel" />
               </children>
            </HBox>
            <Label fx:id="txt_emptyName" text="Label" />
         </children>
      </VBox>
   </children>
</AnchorPane>
