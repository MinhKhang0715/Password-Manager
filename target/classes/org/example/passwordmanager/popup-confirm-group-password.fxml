<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="202.0" prefWidth="454.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.passwordmanager.UserInterface.PopupConfirmGroupPassword">
   <children>
      <VBox alignment="CENTER" layoutX="29.0" layoutY="49.0" prefHeight="35.0" prefWidth="396.0">
         <children>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="396.0" spacing="50.0">
               <children>
                  <Label text="Enter group password" />
                  <PasswordField fx:id="txt_password" promptText="Group password" />
               </children>
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" prefHeight="4.0" prefWidth="396.0" spacing="150.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onConfirmButtonClick" text="Confirm" />
                  <Button mnemonicParsing="false" onAction="#onCancelButtonClick" text="Cancel" />
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </HBox>
            <Label fx:id="lbl_wrongPassword" />
         </children>
      </VBox>
   </children>
</AnchorPane>
